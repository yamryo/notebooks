   "source": [
    "z, c, e = var('z','c', 'e')\n",
    "c, e = (-1, -1)\n",
    "\n",
    "cp = 1+c*z^2+e*z^4; display(cp)\n",
    "\n",
    "t = var('t')\n",
    "ap = cp.subs({z: t^(-1/2)-t^(1/2)})*t^(cp.degree(z)/2)\n",
    "display(ap.expand().simplify().collect(t))"
   ]
